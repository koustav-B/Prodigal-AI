      "f(x): x**3 + 2*x**2 + 3*x + 1\n",
      "f'(x): 3*x**2 + 4*x + 3\n",
      "f''(x): 2*(3*x + 2)\n"
     ]
    }
   ],
   "source": [
    "from sympy import symbols, diff\n",
    "\n",
    "x = symbols('x')\n",
    "f = x**3 + 2*x**2 + 3*x + 1\n",
    "\n",
    "# First derivative\n",
    "df = diff(f, x)\n",
    "\n",
    "# Second derivative\n",
    "d2f = diff(f, x, 2)\n",
    "\n",
    "print(\"f(x):\", f)\n",
    "print(\"f'(x):\", df)\n",
    "print(\"f''(x):\", d2f)\n"
   ]
  }
