    "import random\n",
    "\n",
    "# Probability example: rolling a die\n",
    "trials = 10000\n",
    "count = 0\n",
    "\n",
    "for _ in range(trials):\n",
    "    if random.randint(1, 6) == 6:\n",
    "        count += 1\n",
    "\n",
    "prob = count / trials\n",
    "print(\"P(rolling a 6):\", prob)\n"
   ]
  }
